{
  "expo": {
    "name": "Lune",
    "slug": "lune-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lune-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/lune-splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2b311a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lune-adaptive-icon.png",
        "backgroundColor": "#2b311a"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}